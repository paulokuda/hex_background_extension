{
  "manifest_version": 2,
  "name": "Color My World",
  "description": "Displays time and the corresponding hex as background",
  "version": "1.4",
  "chrome_url_overrides" : {
    "newtab": "background.html"
  },
  "permissions": [
    "<all_urls>",
    "chrome://favicon/",
    "management",
    "geolocation",
    "storage"
  ]
}
